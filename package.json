  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.14",
    "nodemailer": "^6.4.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "uuid": "^3.4.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.1"
  }
}
